{
	"name": "HitCounters",
	"namemsg": "hitcounters-extensionname",
	"version": "0.5.7",
	"author": [
		"[//mwstake.org Mark A. Hershberger]",
		"[//wikimannia.org WikiMANNia]"
	],
	"url": "https://github.com/WikiMANNia/Mediawiki-Extension-HitCounters",
	"descriptionmsg": "hitcounters-desc",
	"type": "variable",
	"license-name": "GPL-3.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.25.0"
	},
	"MessagesDirs": {
		"HitCounters": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"HitCountersMagic": "HitCounters.i18n.magic.php",
		"HitCountersAlias": "HitCounters.i18n.alias.php"
	},
	"SpecialPages": {
		"PopularPages": "MediaWiki\\Extension\\HitCounters\\SpecialPopularPages"
	},
	"Hooks": {
		"GetPreferences": "MediaWiki\\Extension\\HitCounters\\Hooks::onGetPreferences",
		"SpecialStatsAddExtra": "MediaWiki\\Extension\\HitCounters\\Hooks::onSpecialStatsAddExtra",
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\HitCounters\\Hooks::onLoadExtensionSchemaUpdates",
		"MagicWordwgVariableIDs": "MediaWiki\\Extension\\HitCounters\\Hooks::onMagicWordwgVariableIDs",
		"ParserGetVariableValueSwitch": "MediaWiki\\Extension\\HitCounters\\Hooks::onParserGetVariableValueSwitch",
		"ParserFirstCallInit": "MediaWiki\\Extension\\HitCounters\\Hooks::onParserFirstCallInit",
		"PageViewUpdates": "MediaWiki\\Extension\\HitCounters\\Hooks::onPageViewUpdates",
		"SkinTemplateOutputPageBeforeExec": "MediaWiki\\Extension\\HitCounters\\Hooks::onSkinTemplateOutputPageBeforeExec",
		"AbuseFilter-builder": "MediaWiki\\Extension\\HitCounters\\Hooks::onAbuseFilterBuilder",
		"AbuseFilter-deprecatedVariables": "MediaWiki\\Extension\\HitCounters\\Hooks::onAbuseFilterDeprecatedVariables",
		"AbuseFilter-generateTitleVars": "MediaWiki\\Extension\\HitCounters\\Hooks::onAbuseFilterGenerateTitleVars",
		"AbuseFilter-computeVariable": "MediaWiki\\Extension\\HitCounters\\Hooks::onAbuseFilterComputeVariable"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\HitCounters\\DBConnect": "includes/DBConnect.php",
		"MediaWiki\\Extension\\HitCounters\\Hooks": "includes/Hooks.php",
		"MediaWiki\\Extension\\HitCounters\\HitCounters": "includes/HitCounters.php",
		"MediaWiki\\Extension\\HitCounters\\ViewCountUpdate": "includes/ViewCountUpdate.php",
		"MediaWiki\\Extension\\HitCounters\\SpecialPopularPages": "includes/SpecialPopularPages.php",
		"MediaWiki\\Extension\\HitCounters\\HCUpdater": "includes/HCUpdater.php"
	},
	"config": {
		"HitcounterUpdateFreq": 1,
		"DisableCounters": false,
		"EnableCountersAtTheFooter": false
	},
	"DefaultUserOptions": {
		"hitcounters-exempt": false,
		"hitcounters-pageid": false,
		"hitcounters-textlength": false,
		"hitcounters-numberofmostviewedpages": 50
	},
	"manifest_version": 1
}
